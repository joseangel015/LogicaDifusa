\begin_inset Quotes qld
\end_inset

si - entonces
\begin_inset Quotes qrd
\end_inset

 para la tabla de inferencia 
\begin_inset CommandInset ref
LatexCommand ref
reference "tab:tabla2"
plural "false"
caps "false"
noprefix "false"

\end_inset

:
\end_layout

