\end_layout

\begin_layout Subsection*
Código para la generación de gráficas de los conjuntos de entrada y salida
 caracterizados {graficas.py}
\end_layout

\begin_layout Standard
import numpy as np
\end_layout

\begin_layout Standard
import matplotlib.pyplot as plt 
\end_layout

\begin_layout Standard
x = np.arange(0, 50, 0.1) 
\end_layout

\begin_layout Standard
y = np.arange(0, 100, 0.1) 
\end_layout

\begin_layout Standard
z1 = np.arange(15, 30, 0.1)
\end_layout

\begin_layout Standard
z2 = np.arange(0, 3, 0.01)
\end_layout

\begin_layout Standard
# X, Y = np.meshgrid(x, y)
\end_layout

\begin_layout Standard
#Temperatura seca
\end_layout

\begin_layout Standard
A1 = np.zeros(len(x)) #Frio
\end_layout

\begin_layout Standard
A2 = np.zeros(len(x)) #Templado
\end_layout

\begin_layout Standard
A3 = np.zeros(len(x)) #Caliente
\end_layout

\begin_layout Standard
A4 = np.zeros(len(x)) #Muy caliente
\end_layout

\begin_layout Standard
#Humedad relativa
\end_layout

\begin_layout Standard
B1 = np.zeros(len(y)) #Seco
\end_layout

\begin_layout Standard
B2 = np.zeros(len(y)) #Zona de comfort
\end_layout

\begin_layout Standard
B3 = np.zeros(len(y)) #Húmedo
\end_layout

\begin_layout Standard
#Temperatura del aire
\end_layout

\begin_layout Standard
C1 = np.zeros(len(z1)) #Frio
\end_layout

\begin_layout Standard
C2 = np.zeros(len(z1)) #Templado
\end_layout

\begin_layout Standard
C3 = np.zeros(len(z1)) #Caliente
\end_layout

\begin_layout Standard
#Velocidad del viento
\end_layout

\begin_layout Standard
D1 = np.zeros(len(z2)) #Calma
\end_layout

\begin_layout Standard
D2 = np.zeros(len(z2)) #Aire ligero
\end_layout

\begin_layout Standard
D3 = np.zeros(len(z2)) #Brisa ligera
\end_layout

\begin_layout Standard
D4 = np.zeros(len(z2)) #Brisa suave
\end_layout

\begin_layout Standard
def funcionCampana(a,b,c,C,r):
\end_layout

\begin_layout Standard
a = a*10 #Ancho de la campana
\end_layout

\begin_layout Standard
b = b*10 #Razon de cambio
\end_layout

\begin_layout Standard
c = c*10 #Centro de la campana
\end_layout

\begin_layout Standard
for i in range(len(r)):
\end_layout

\begin_layout Standard
C[i] = 1/(1 + abs((i-c)/a)**(2*b))
\end_layout

\begin_layout Standard
return C
\end_layout

\begin_layout Standard
def funcionTrapezoidal(a,b,c,d,C,r):
\end_layout

\begin_layout Standard
a = a*10
\end_layout

\begin_layout Standard
b = b*10
\end_layout

\begin_layout Standard
c = c*10
\end_layout

\begin_layout Standard
d = d*10
\end_layout

\begin_layout Standard
for i in range(len(r)):
\end_layout

\begin_layout Standard
if i <= a:
\end_layout

\begin_layout Standard
C[i] = 0
\end_layout

\begin_layout Standard
elif a < i and i <= b:
\end_layout

\begin_layout Standard
C[i] = (i - a)/(b - a)
\end_layout

\begin_layout Standard
elif b < i and i <= c:
\end_layout

\begin_layout Standard
C[i] = 1
\end_layout

\begin_layout Standard
elif c < i and i <= d:
\end_layout

\begin_layout Standard
C[i] = (d - i)/(d - c)
\end_layout

\begin_layout Standard
elif d < i:
\end_layout

\begin_layout Standard
C[i] = 0
\end_layout

\begin_layout Standard
return C
\end_layout

\begin_layout Standard
#Generación de los conjuntos difusos
\end_layout

\begin_layout Standard
#Temperatura seca
\end_layout

\begin_layout Standard
A1 = funcionCampana(10,0.25,0,A1,x)
\end_layout

\begin_layout Standard
A2 = funcionCampana(10,0.25,15,A2,x)
\end_layout

\begin_layout Standard
A3 = funcionCampana(10,0.25,35,A3,x)
\end_layout

\begin_layout Standard
A4 = funcionCampana(10,0.25,50,A4,x)
\end_layout

\begin_layout Standard
plt.figure(1)
\end_layout

\begin_layout Standard
plt.plot(x,A1,x,A2,x,A3,x,A4)
\end_layout

\begin_layout Standard
plt.xlabel("Temperatura seca [°C]")
\end_layout

\begin_layout Standard
plt.ylabel("Membresía")
\end_layout

\begin_layout Standard
#Humedad relativa
\end_layout

\begin_layout Standard
B1 = funcionTrapezoidal(-1,-1,20,30,B1,y)
\end_layout

\begin_layout Standard
B2 = funcionTrapezoidal(20,30,70,80,B2,y)
\end_layout

\begin_layout Standard
B3 = funcionTrapezoidal(70,80,100,100,B3,y)
\end_layout

\begin_layout Standard
plt.figure(2)
\end_layout

\begin_layout Standard
plt.plot(y,B1,y,B2,y,B3)
\end_layout

\begin_layout Standard
plt.xlabel("Humedad relativa")
\end_layout

\begin_layout Standard
plt.ylabel("Membresía")
\end_layout

\begin_layout Standard
# Temperatura del viento
\end_layout

\begin_layout Standard
C1 = funcionCampana(5,0.25,0,C1,z1)
\end_layout

\begin_layout Standard
C2 = funcionCampana(5,0.25,7.5,C2,z1)
\end_layout

\begin_layout Standard
C3 = funcionCampana(5,0.25,15,C3,z1)
\end_layout

\begin_layout Standard
plt.figure(3)
\end_layout

\begin_layout Standard
plt.plot(z1,C1,z1,C2,z1,C3)
\end_layout

\begin_layout Standard
plt.xlabel("Temperatura del viento [°C]")
\end_layout

\begin_layout Standard
plt.ylabel("Membresía")
\end_layout

\begin_layout Standard
#Velocidad del viento
\end_layout

\begin_layout Standard
D1 = funcionTrapezoidal(-1*10,-1*10,0.37*10,0.62*10,D1,z2)
\end_layout

\begin_layout Standard
D2 = funcionTrapezoidal(0.37*10,0.62*10,1.37*10,1.62*10,D2,z2)
\end_layout

\begin_layout Standard
D3 = funcionTrapezoidal(1.37*10,1.62*10,2.37*10,2.62*10,D3,z2)
\end_layout

\begin_layout Standard
D4 = funcionTrapezoidal(2.37*10,2.62*10,3*10,3*10,D4,z2)
\end_layout

\begin_layout Standard
plt.figure(4)
\end_layout

\begin_layout Standard
plt.plot(z2,D1,z2,D2,z2,D3,z2,D4)
\end_layout

\begin_layout Standard
plt.xlabel("Velocidad del viento [m/s]")
\end_layout

\begin_layout Standard
plt.ylabel("Membresía")
\end_layout

\begin_layout Standard
#CZ = np.zeros((len(x),len(y)))
\end_layout

\begin_layout Standard
#Obtención de los grados de pertenencia a los conjuntos de los universos
 de entrada
\end_layout

\begin_layout Standard
valx = int(input("Ingrese el valor de la temperatura seca (0 - 50): "))
\end_layout

\begin_layout Standard
memA = [A1[valx*10],A2[valx*10],A3[valx*10],A4[valx*10]]
\end_layout

\begin_layout Standard
valy = int(input("Ingrese el valor de la humedad relativa (0 - 100): "))
\end_layout

\begin_layout Standard
memB = [B1[valy*10],B2[valy*10],B3[valy*10]]
\end_layout

\begin_layout Standard
#Composición max - min (Evaluación de las reglas "si-entonces")
\end_layout

\begin_layout Standard
#Tablas de inferencia (intersección - min)
\end_layout

\begin_layout Standard
tabla = [[np.minimum(memA[0],memB[0]),np.minimum(memA[1],memB[0]),
\end_layout

\begin_layout Standard
np.minimum(memA[2],memB[0]),np.minimum(memA[3],memB[0])],
\end_layout

\begin_layout Standard
[np.minimum(memA[0],memB[1]),np.minimum(memA[1],memB[1]),
\end_layout

\begin_layout Standard
np.minimum(memA[2],memB[1]),np.minimum(memA[3],memB[1])],
\end_layout

\begin_layout Standard
[np.minimum(memA[0],memB[2]),np.minimum(memA[1],memB[2]),
\end_layout

\begin_layout Standard
np.minimum(memA[2],memB[2]),np.minimum(memA[3],memB[2])]]
\end_layout

\begin_layout Standard
print("Tabla de inferencia: ",str(tabla))
\end_layout

\begin_layout Standard
#Agregación - max hacia Temperatura del viento
\end_layout

\begin_layout Standard
agregacionC1 = max(tabla[0][2],max(tabla[0][3],max(tabla[1][3],
\end_layout

\begin_layout Standard
max(tabla[2][2],tabla[2][3])))) #Frio
\end_layout

\begin_layout Standard
agregacionC2 = max(tabla[0][1],max(tabla[1][0],max(tabla[1][1],
\end_layout

\begin_layout Standard
max(tabla[1][2],tabla[2][1])))) #Templado
\end_layout

\begin_layout Standard
agregacionC3 = max(tabla[0][0],tabla[2][0]) #Caliente
\end_layout

\begin_layout Standard
#Agregación - max hacia Velocidad del viento
\end_layout

\begin_layout Standard
agregacionD1 = tabla[1][1] #Calma
\end_layout

\begin_layout Standard
agregacionD2 = max(tabla[0][0],max(tabla[0][1],max(tabla[1][0],
\end_layout

\begin_layout Standard
max(tabla[2][0],tabla[2][1])))) #Aire ligero
\end_layout

\begin_layout Standard
agregacionD3 = max(tabla[0][2],max(tabla[1][2],tabla[2][2])) #Brisa ligera
\end_layout

\begin_layout Standard
agregacionD4 = max(tabla[0][3],max(tabla[1][3],tabla[2][3])) #Brisa suave
\end_layout

\begin_layout Standard
aux1 = agregacionC1*np.ones(len(C1))
\end_layout

\begin_layout Standard
aux2 = agregacionC2*np.ones(len(C2))
\end_layout

\begin_layout Standard
aux3 = agregacionC3*np.ones(len(C3))
\end_layout

\begin_layout Standard
aux4 = agregacionD1*np.ones(len(D1))
\end_layout

\begin_layout Standard
aux5 = agregacionD2*np.ones(len(D2))
\end_layout

\begin_layout Standard
aux6 = agregacionD3*np.ones(len(D3))
\end_layout

\begin_layout Standard
aux7 = agregacionD4*np.ones(len(D4))
\end_layout

\begin_layout Standard
C1_r = np.minimum(C1,aux1)
\end_layout

\begin_layout Standard
C2_r = np.minimum(C2,aux2)
\end_layout

\begin_layout Standard
C3_r = np.minimum(C3,aux3)
\end_layout

\begin_layout Standard
D1_r = np.minimum(D1,aux4)
\end_layout

\begin_layout Standard
D2_r = np.minimum(D2,aux5)
\end_layout

\begin_layout Standard
D3_r = np.minimum(D3,aux6)
\end_layout

\begin_layout Standard
D4_r = np.minimum(D4,aux7)
\end_layout

\begin_layout Standard
plt.figure(5)
\end_layout

\begin_layout Standard
plt.plot(z1,C1,z1,C2,z1,C3,z1,C1_r,z1,C2_r,z1,C3_r)
\end_layout

\begin_layout Standard
plt.xlabel("Temperatura del viento [°C]")
\end_layout

\begin_layout Standard
plt.ylabel("Membresía")
\end_layout

\begin_layout Standard
plt.figure(6)
\end_layout

\begin_layout Standard
plt.plot(z2,D1,z2,D2,z2,D3,z2,D4,z2,D1_r,z2,D2_r,z2,D3_r,z2,D4_r)
\end_layout

\begin_layout Standard
plt.xlabel("Velocidad del viento [m/s]")
\end_layout

\begin_layout Standard
plt.ylabel("Membresía") 
\end_layout

\begin_layout Standard
#Dedifusificación
\end_layout

\begin_layout Standard
#Temperatura del viento
\end_layout

\begin_layout Standard
z_d = 0
\end_layout

\begin_layout Standard
nume = 0
\end_layout

\begin_layout Standard
deno = 0
\end_layout

\begin_layout Standard
CT1 = np.maximum(C1_r,C2_r,C3_r)
\end_layout

\begin_layout Standard
plt.figure(7)
\end_layout

\begin_layout Standard
plt.plot(z1,CT1)
\end_layout

\begin_layout Standard
plt.xlabel("Temperatura del viento [°C]")
\end_layout

\begin_layout Standard
plt.ylabel("Membresía")
\end_layout

\begin_layout Standard
for i in range(len(z1)):
\end_layout

\begin_layout Standard
nume += CT1[i]*(i/10+15)
\end_layout

\begin_layout Standard
deno += CT1[i]
\end_layout

\begin_layout Standard
z_d = nume/deno
\end_layout

\begin_layout Standard
print("z1* = ", str(z_d), " [°C]")
\end_layout

\begin_layout Standard
# Velocidad del viento
\end_layout

\begin_layout Standard
z_d = 0
\end_layout

\begin_layout Standard
nume = 0
\end_layout

\begin_layout Standard
deno = 0
\end_layout

\begin_layout Standard
CT2 = np.maximum(np.maximum(D1_r,D2_r,D3_r),D4_r)
\end_layout

\begin_layout Standard
plt.figure(8)
\end_layout

\begin_layout Standard
plt.plot(z2,CT2)
\end_layout

\begin_layout Standard
plt.xlabel("Velocidad del viento [m/s]")
\end_layout

\begin_layout Standard
plt.ylabel("Membresía")
\end_layout

\begin_layout Standard
for i in range(len(z2)):
\end_layout

\begin_layout Standard
nume += CT2[i]*i/100
\end_layout

\begin_layout Standard
deno += CT2[i]
\end_layout

\begin_layout Standard
z_d = nume/deno
\end_layout

\begin_layout Standard
print("z2* = ", str(z_d), "[m/s]")
\end_layout

\begin_layout Subsection*
Código para la generación de superficies de control {3d.py}
\end_layout

\begin_layout Standard
import numpy as np
\end_layout

\begin_layout Standard
import matplotlib.pyplot as plt 
\end_layout

\begin_layout Standard
from matplotlib import cm
\end_layout

\begin_layout Standard
x = np.arange(0, 50, 0.1) 
\end_layout

\begin_layout Standard
y = np.arange(0, 100, 0.2) 
\end_layout

\begin_layout Standard
z1 = np.arange(15, 30, 0.1)
\end_layout

\begin_layout Standard
z2 = np.arange(0, 3, 0.01)
\end_layout

\begin_layout Standard
X, Y = np.meshgrid(x, y)
\end_layout

\begin_layout Standard
#Temperatura seca
\end_layout

\begin_layout Standard
A1 = np.zeros(len(x)) #Frio
\end_layout

\begin_layout Standard
A2 = np.zeros(len(x)) #Templado
\end_layout

\begin_layout Standard
A3 = np.zeros(len(x)) #Caliente
\end_layout

\begin_layout Standard
A4 = np.zeros(len(x)) #Muy caliente
\end_layout

\begin_layout Standard
#Humedad relativa
\end_layout

\begin_layout Standard
B1 = np.zeros(len(y)) #Seco
\end_layout

\begin_layout Standard
B2 = np.zeros(len(y)) #Zona de comfort
\end_layout

\begin_layout Standard
B3 = np.zeros(len(y)) #Húmedo
\end_layout

\begin_layout Standard
#Temperatura del aire
\end_layout

\begin_layout Standard
C1 = np.zeros(len(z1)) #Frio
\end_layout

\begin_layout Standard
C2 = np.zeros(len(z1)) #Templado
\end_layout

\begin_layout Standard
C3 = np.zeros(len(z1)) #Caliente
\end_layout

\begin_layout Standard
#Velocidad del viento
\end_layout

\begin_layout Standard
D1 = np.zeros(len(z2)) #Calma
\end_layout

\begin_layout Standard
D2 = np.zeros(len(z2)) #Aire ligero
\end_layout

\begin_layout Standard
D3 = np.zeros(len(z2)) #Brisa ligera
\end_layout

\begin_layout Standard
D4 = np.zeros(len(z2)) #Brisa suave
\end_layout

\begin_layout Standard
def funcionCampana(a,b,c,C,r):
\end_layout

\begin_layout Standard
a = a*10 #Ancho de la campana
\end_layout

\begin_layout Standard
b = b*10 #Razon de cambio
\end_layout

\begin_layout Standard
c = c*10 #Centro de la campana
\end_layout

\begin_layout Standard
for i in range(len(r)):
\end_layout

\begin_layout Standard
C[i] = 1/(1 + abs((i-c)/a)**(2*b))
\end_layout

\begin_layout Standard
return C
\end_layout

\begin_layout Standard
def funcionTrapezoidal(a,b,c,d,C,r):
\end_layout

\begin_layout Standard
a = a*10
\end_layout

\begin_layout Standard
b = b*10
\end_layout

\begin_layout Standard
c = c*10
\end_layout

\begin_layout Standard
d = d*10
\end_layout

\begin_layout Standard
for i in range(len(r)):
\end_layout

\begin_layout Standard
if i <= a:
\end_layout

\begin_layout Standard
C[i] = 0
\end_layout

\begin_layout Standard
elif a < i and i <= b:
\end_layout

\begin_layout Standard
C[i] = (i - a)/(b - a)
\end_layout

\begin_layout Standard
elif b < i and i <= c:
\end_layout

\begin_layout Standard
C[i] = 1
\end_layout

\begin_layout Standard
elif c < i and i <= d:
\end_layout

\begin_layout Standard
C[i] = (d - i)/(d - c)
\end_layout

\begin_layout Standard
elif d < i:
\end_layout

\begin_layout Standard
C[i] = 0
\end_layout

\begin_layout Standard
return C
\end_layout

\begin_layout Standard
#Generación de los conjuntos difusos
\end_layout

\begin_layout Standard
#Temperatura seca
\end_layout

\begin_layout Standard
A1 = funcionCampana(10,0.25,0,A1,x)
\end_layout

\begin_layout Standard
A2 = funcionCampana(10,0.25,15,A2,x)
\end_layout

\begin_layout Standard
A3 = funcionCampana(10,0.25,35,A3,x)
\end_layout

\begin_layout Standard
A4 = funcionCampana(10,0.25,50,A4,x)
\end_layout

\begin_layout Standard
#Humedad relativa
\end_layout

\begin_layout Standard
B1 = funcionTrapezoidal(-1,-1,20,30,B1,y)
\end_layout

\begin_layout Standard
B2 = funcionTrapezoidal(20,30,70,80,B2,y)
\end_layout

\begin_layout Standard
B3 = funcionTrapezoidal(70,80,100,100,B3,y)
\end_layout

\begin_layout Standard
# Temperatura del viento
\end_layout

\begin_layout Standard
C1 = funcionCampana(5,0.25,0,C1,z1)
\end_layout

\begin_layout Standard
C2 = funcionCampana(5,0.25,7.5,C2,z1)
\end_layout

\begin_layout Standard
C3 = funcionCampana(5,0.25,15,C3,z1)
\end_layout

\begin_layout Standard
#Velocidad del viento
\end_layout

\begin_layout Standard
D1 = funcionTrapezoidal(-1*10,-1*10,0.37*10,0.62*10,D1,z2)
\end_layout

\begin_layout Standard
D2 = funcionTrapezoidal(0.37*10,0.62*10,1.37*10,1.62*10,D2,z2)
\end_layout

\begin_layout Standard
D3 = funcionTrapezoidal(1.37*10,1.62*10,2.37*10,2.62*10,D3,z2)
\end_layout

\begin_layout Standard
D4 = funcionTrapezoidal(2.37*10,2.62*10,3*10,3*10,D4,z2)
\end_layout

\begin_layout Standard
CZ1 = np.zeros((len(x),len(y)))
\end_layout

\begin_layout Standard
CZ2 = np.zeros((len(x),len(y)))
\end_layout

\begin_layout Standard
#Obtención de los grados de pertenencia a los conjuntos de los universos
 de entrada
\end_layout

\begin_layout Standard
def difusificacion(valx,valy):
\end_layout

\begin_layout Standard
memA = [A1[valx],A2[valx],A3[valx],A4[valx]]
\end_layout

\begin_layout Standard
memB = [B1[valy],B2[valy],B3[valy]]
\end_layout

\begin_layout Standard
#Composición max - min (Evaluación de las reglas "si-entonces")
\end_layout

\begin_layout Standard
#Tablas de inferencia (intersección - min)
\end_layout

\begin_layout Standard
tabla = [[np.minimum(memA[0],memB[0]),np.minimum(memA[1],
\end_layout

\begin_layout Standard
memB[0]),np.minimum(memA[2],memB[0]),np.minimum(memA[3],memB[0])],
\end_layout

\begin_layout Standard
[np.minimum(memA[0],memB[1]),np.minimum(memA[1],
\end_layout

\begin_layout Standard
memB[1]),np.minimum(memA[2],memB[1]),np.minimum(memA[3],memB[1])],
\end_layout

\begin_layout Standard
[np.minimum(memA[0],memB[2]),np.minimum(memA[1],
\end_layout

\begin_layout Standard
memB[2]),np.minimum(memA[2],memB[2]),np.minimum(memA[3],memB[2])]]
\end_layout

\begin_layout Standard
#Agregación - max hacia Temperatura del viento
\end_layout

\begin_layout Standard
agregacionC1 = max(tabla[0][2],max(tabla[0][3],
\end_layout

\begin_layout Standard
max(tabla[1][3],max(tabla[2][2],tabla[2][3])))) #Frio
\end_layout

\begin_layout Standard
agregacionC2 = max(tabla[0][1],max(tabla[1][0],
\end_layout

\begin_layout Standard
max(tabla[1][1],max(tabla[1][2],tabla[2][1])))) #Templado
\end_layout

\begin_layout Standard
agregacionC3 = max(tabla[0][0],tabla[2][0]) #Caliente
\end_layout

\begin_layout Standard
#Agregación - max hacia Velocidad del viento
\end_layout

\begin_layout Standard
agregacionD1 = tabla[1][1] #Calma
\end_layout

\begin_layout Standard
agregacionD2 = max(tabla[0][0],max(tabla[0][1],
\end_layout

\begin_layout Standard
max(tabla[1][0],max(tabla[2][0],tabla[2][1])))) #Aire ligero
\end_layout

\begin_layout Standard
agregacionD3 = max(tabla[0][2],max(tabla[1][2],tabla[2][2])) #Brisa ligera
\end_layout

\begin_layout Standard
agregacionD4 = max(tabla[0][3],max(tabla[1][3],tabla[2][3])) #Brisa suave
\end_layout

\begin_layout Standard
aux1 = agregacionC1*np.ones(len(C1))
\end_layout

\begin_layout Standard
aux2 = agregacionC2*np.ones(len(C2))
\end_layout

\begin_layout Standard
aux3 = agregacionC3*np.ones(len(C3))
\end_layout

\begin_layout Standard
aux4 = agregacionD1*np.ones(len(D1))
\end_layout

\begin_layout Standard
aux5 = agregacionD2*np.ones(len(D2))
\end_layout

\begin_layout Standard
aux6 = agregacionD3*np.ones(len(D3))
\end_layout

\begin_layout Standard
aux7 = agregacionD4*np.ones(len(D4))
\end_layout

\begin_layout Standard
aux1 = agregacionC1*np.ones(len(C1))
\end_layout

\begin_layout Standard
aux2 = agregacionC2*np.ones(len(C2))
\end_layout

\begin_layout Standard
aux3 = agregacionC3*np.ones(len(C3))
\end_layout

\begin_layout Standard
aux4 = agregacionD1*np.ones(len(D1))
\end_layout

\begin_layout Standard
aux5 = agregacionD2*np.ones(len(D2))
\end_layout

\begin_layout Standard
aux6 = agregacionD3*np.ones(len(D3))
\end_layout

\begin_layout Standard
C1_r = np.minimum(C1,aux1)
\end_layout

\begin_layout Standard
C2_r = np.minimum(C2,aux2)
\end_layout

\begin_layout Standard
C3_r = np.minimum(C3,aux3)
\end_layout

\begin_layout Standard
D1_r = np.minimum(D1,aux4)
\end_layout

\begin_layout Standard
D2_r = np.minimum(D2,aux5)
\end_layout

\begin_layout Standard
D3_r = np.minimum(D3,aux6)
\end_layout

\begin_layout Standard
D4_r = np.minimum(D4,aux7)
\end_layout

\begin_layout Standard
z_d = np.zeros((1,2))
\end_layout

\begin_layout Standard
nume = 0
\end_layout

\begin_layout Standard
deno = 0
\end_layout

\begin_layout Standard
CT1 = np.maximum(C1_r,C2_r,C3_r)
\end_layout

\begin_layout Standard
CT2 = np.maximum(np.maximum(D1_r,D2_r,D3_r),D4_r)
\end_layout

\begin_layout Standard
for i in range(len(z1)):
\end_layout

\begin_layout Standard
nume += CT1[i]*(i/10+15)
\end_layout

\begin_layout Standard
deno += CT1[i]
\end_layout

\begin_layout Standard
z_d[0,0] = nume/deno
\end_layout

\begin_layout Standard
nume = 0
\end_layout

\begin_layout Standard
deno = 0
\end_layout

\begin_layout Standard
for i in range(len(z2)):
\end_layout

\begin_layout Standard
nume += CT2[i]*i/100
\end_layout

\begin_layout Standard
deno += CT2[i]
\end_layout

\begin_layout Standard
z_d[0,1] = nume/deno
\end_layout

\begin_layout Standard
return z_d
\end_layout

\begin_layout Standard
for i in range(len(x)):
\end_layout

\begin_layout Standard
for j in range(len(y)):
\end_layout

\begin_layout Standard
CZ1[i,j] = difusificacion(i,j)[0][0]
\end_layout

\begin_layout Standard
CZ2[i,j] = difusificacion(i,j)[0][1]
\end_layout

\begin_layout Standard
fig, ax = plt.subplots(subplot_kw={"projection": "3d"})
\end_layout

\begin_layout Standard
ax.set_xlabel('Y')
\end_layout

\begin_layout Standard
ax.set_ylabel('X')
\end_layout

\begin_layout Standard
ax.set_zlabel('Z')
\end_layout

\begin_layout Standard
surf = ax.plot_surface(X, Y, CZ1, cmap=cm.coolwarm,
\end_layout

\begin_layout Standard
linewidth=0, antialiased=False)
\end_layout

\begin_layout Standard
fig2, ax2 = plt.subplots(subplot_kw={"projection": "3d"})
\end_layout

\begin_layout Standard
ax2.set_xlabel('Y')
\end_layout

\begin_layout Standard
ax2.set_ylabel('X')
\end_layout

\begin_layout Standard
ax2.set_zlabel('Z')
\end_layout

\begin_layout Standard
surf = ax2.plot_surface(X, Y, CZ2, cmap=cm.coolwarm,
\end_layout

\begin_layout Standard
linewidth=0, antialiased=False)
\end_layout

