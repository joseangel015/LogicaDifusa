\begin_layout Standard
En base a la tabla de inferencia 
\begin_inset CommandInset ref
LatexCommand ref
reference "tab:tabla1"
plural "false"
caps "false"
noprefix "false"

\end_inset

 propongo las siguientes reglas 
\begin_inset Quotes qld
\end_inset

si - entonces
\begin_inset Quotes qrd
\end_inset

:
\end_layout

\begin_layout Enumerate
Si el aire es seco y su temperatura fría, entonces la temperatura de salida
 es Caliente.
\end_layout

\begin_layout Enumerate
Si el aire es seco y su temperatura es templada, entonces la temperatura
 de salida es Templada.
\end_layout

\begin_layout Enumerate
Si el aire es seco y su temperatura es caliente , entonces la temperatura
 de salida es Fría.
\end_layout

\begin_layout Enumerate
Si el aire es seco y su temperatura es muy caliente , entonces la temperatura
 de salida es Fría.
\end_layout

\begin_layout Enumerate
Si el aire está en zona de comfort y su temperatura fría, entonces la temperatur
a de salida es Templada.
\end_layout

\begin_layout Enumerate
Si el aire está en zona de comfort y su temperatura templada, entonces la
 temperatura de salida es Templada.
\end_layout

\begin_layout Enumerate
Si el aire está en zona de comfort y su temperatura caliente, entonces la
 temperatura de salida es Templada.
\end_layout

\begin_layout Enumerate
Si el aire está en zona de comfort y su temperatura muy caliente, entonces
 la temperatura de salida es Fría.
\end_layout

\begin_layout Enumerate
Si el aire es húmedo y su temperatura fría, entonces la temperatura de salida
 es Caliente.
\end_layout

\begin_layout Enumerate
Si el aire es húmedo y su temperatura es templada, entonces la temperatura
 de salida es Templada.
\end_layout

\begin_layout Enumerate
Si el aire es húmedo y su temperatura es caliente , entonces la temperatura
 de salida es Fría.
\end_layout

\begin_layout Enumerate
Si el aire es húmedo y su temperatura es muy caliente , entonces la temperatura
 de salida es Fría.
\end_layout

\begin_layout Standard
